	<queryString>
		<![CDATA[SELECT DISTINCT(VOUCHERHEADER.VOUCHERNUMBER) AS VOUCHER_NUMBER,
VOUCHERHEADER.VOUCHERDATE AS VOUCHER_DATE,
FUND.NAME AS FUND_NAME,
VOUCHERHEADER.TYPE AS VOUCHER_TYPE,
EGCL_SERVICEDETAILS.NAME AS SERVICE_NAME, 
BANK.NAME||'-'|| BANKACCOUNT.ACCOUNTNUMBER as BANK_ACCOUNT_NUMBER,
GENERALLEDGER.DEBITAMOUNT AS VOUCHER_AMOUNT,
EG_USER.USERNAME AS REMITTED_BY
FROM 
VOUCHERHEADER VOUCHERHEADER INNER JOIN GENERALLEDGER GENERALLEDGER ON VOUCHERHEADER.ID = GENERALLEDGER.VOUCHERHEADERID 
INNER JOIN FUND FUND ON FUND.ID = VOUCHERHEADER.FUNDID
INNER JOIN EGF_INSTRUMENTOTHERDETAILS EGF_INSTRUMENTOTHERDETAILS ON EGF_INSTRUMENTOTHERDETAILS.PAYINSLIPID = VOUCHERHEADER.ID
INNER JOIN EGF_INSTRUMENTHEADER EGF_INSTRUMENTHEADER ON EGF_INSTRUMENTHEADER.ID = EGF_INSTRUMENTOTHERDETAILS.INSTRUMENTHEADERID
INNER JOIN EGCL_COLLECTIONINSTRUMENT EGCL_COLLECTIONINSTRUMENT ON EGCL_COLLECTIONINSTRUMENT.INSTRUMENTHEADER = EGF_INSTRUMENTHEADER.ID
INNER JOIN EGCL_COLLECTIONHEADER EGCL_COLLECTIONHEADER ON EGCL_COLLECTIONHEADER.ID = EGCL_COLLECTIONINSTRUMENT.COLLECTIONHEADER
INNER JOIN EGCL_SERVICEDETAILS EGCL_SERVICEDETAILS ON EGCL_SERVICEDETAILS.ID = EGCL_COLLECTIONHEADER.SERVICEDETAILS
INNER JOIN CHARTOFACCOUNTS CHARTOFACCOUNTS ON CHARTOFACCOUNTS.GLCODE = GENERALLEDGER.GLCODE
INNER JOIN BANKACCOUNT BANKACCOUNT ON BANKACCOUNT.GLCODEID = CHARTOFACCOUNTS.ID
INNER JOIN BANKBRANCH BANKBRANCH ON BANKBRANCH.ID = BANKACCOUNT.BRANCHID
INNER JOIN BANK BANK ON BANK.ID = BANKBRANCH.BANKID
INNER JOIN EG_USER EG_USER ON EG_USER.ID= VOUCHERHEADER.CREATEDBY
WHERE
     GENERALLEDGER.DEBITAMOUNT <> 0
     AND VOUCHERHEADER.VOUCHERDATE >= $P{EGOV_FROM_DATE}
     AND VOUCHERHEADER.VOUCHERDATE < $P{EGOV_TO_DATE_PLUS_ONE}
     $P!{Q_BANKACCOUNT_ID}
     $P!{Q_CREATEDBY_ID}
     $P!{Q_SERVICE_ID}
     $P!{Q_BRANCH_ID}   
GROUP BY 
     VOUCHERHEADER.VOUCHERDATE, VOUCHERHEADER.VOUCHERNUMBER,VOUCHERHEADER.TYPE,EGCL_SERVICEDETAILS.NAME,FUND.NAME,BANK.NAME||'-'|| BANKACCOUNT.ACCOUNTNUMBER,GENERALLEDGER.DEBITAMOUNT,EG_USER.USERNAME
ORDER BY
     VOUCHER_DATE,VOUCHER_TYPE]]>
	</queryString>
